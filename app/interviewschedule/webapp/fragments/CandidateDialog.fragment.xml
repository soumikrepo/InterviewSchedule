<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
>
    <Dialog
        id="popup"
        draggable="true"
        resizable="true"
    >
        <content>
          
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer />

                <Button
                    type="Accept"
                    text="Start Scheduling"
                    press="saveDialog"
                />

                <Button
                    id="Clear"
                    text="Cancel"
                    type="Reject"
                    press="clearDialog"
                    ariaHasPopup="Dialog"
                />
            </Toolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
